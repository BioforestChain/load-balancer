{
  "scheme": "http",
  "host": "0.0.0.0",
  "port": 8888,
  "servers": [
    {
      "name": "Server A",
      "scheme": "http",
      "host": "127.0.0.1",
      "port": 9800
    },
    {
      "name": "Server B",
      "scheme": "http",
      "host": "127.0.0.1",
      "port": 9801
    }
  ],
  "routes": [
    {
      "mode": ":path",
      "value": "/a/*",
      "to": ["Server A"]
    },
    {
      "mode": ":path",
      "value": "/b/*",
      "to": ["Server B"]
    }
  ]
}
